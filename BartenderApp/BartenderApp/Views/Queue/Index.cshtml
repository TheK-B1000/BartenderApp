<html>
<head>
    <title>Bartender App - Queue</title>
</head>
<body>

    <div class="container">
        <h3>Order Queue</h3>
        <table>
            <thead>
                <tr>
                    <th>Order ID</th>
                    <th>Cocktail Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var order in Model)
                {
                    <tr>
                        <td>@order.ID</td>
                        <td>@order.Cocktail.Name</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

</body>
</html>
